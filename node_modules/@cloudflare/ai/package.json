{
  "name": "@cloudflare/ai",
  "version": "1.0.50",
  "type": "module",
  "description": "Cloudflare's Workers AI SDK",
  "author": "Celso Martinho <celso@cloudflare.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "test-prep": "cd test; rm -fr dist; cp -R ../src dist",
    "build": "tsc && esbuild src/index.ts --bundle --outfile=dist/index.js --sourcemap --format=esm && yarn replace-version",
    "build:for-dashboard": "npm_package_version=$npm_package_version bash ./scripts/build-for-dashboard.sh",
    "prepublish": "bash ./scripts/add_registry_config.sh && yarn build",
    "test": "npm run test-prep && NODE_NO_WARNINGS=1 mocha --require ts-node/register --config ./test/mocharc.json --bail ./test/sdk",
    "test-rest": "npm run test-prep && NODE_NO_WARNINGS=1 mocha --config ./test/mocharc.json --bail ./test/rest",
    "dry-publish": "npm pack --dry-run",
    "replace-version": "find ./dist/ -type f -name \"*.js\" -print0 | xargs -0 sed -i -e \"s/@version@/$npm_package_version/g\"",
    "prepare": "husky install"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/mustache": "^4.2.4",
    "@types/node": "^20.3.3",
    "@types/semver": "^7.5.6",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.20.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "mustache": "^4.2.0",
    "prettier": "^2.8.8",
    "semver": "^7.6.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  }
}
